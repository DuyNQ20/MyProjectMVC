<section class="flat-imagebox">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="product-tab">
                    <ul class="tab-list">
                        <li class="active">Sản phẩm mới</li>
                        <li>Nổi bật</li>
                        <li>Bán chạy</li>
                    </ul>
                </div><!-- /.product-tab -->
            </div><!-- /.col-md-12 -->
        </div><!-- /.row -->
        <div class="box-product">
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-lg-3 col-sm-6">
                        <div class="product-box">
                            <div class="imagebox">
                                @if(item.IsNew)
                                {
                                <span class="item-new">NEW</span>
                                }
                                <ul class="box-image owl-carousel-1">
                                    <li>
                                        <a asp-action="Details" asp-route-id="@item.Id" title="">
                                            @{
                                                var image = item.Files.FirstOrDefault(x => x.ProductId == item.Id & x.thumbnail == true);
                                                if (image != null)
                                                {
                                                    <img src="@Url.Content("~/"+image.Path)" />
                                                }
                                            }
                                        </a>
                                    </li>
                                    @{
                                        var image1 = item.Files.FirstOrDefault(x => x.ProductId == item.Id);
                                    }
                                    <li>
                                        <a asp-action="Details" asp-route-id="@item.Id" title="">
                                            @{

                                                if (image1 != null)
                                                {
                                                    <img src="@Url.Content("~/"+image1.Path)" />
                                                }
                                            }
                                        </a>
                                    </li>

                                </ul><!-- /.box-image -->
                                <div class="box-content">
                                    <div class="cat-name">
                                        <a href="~/home/#" title=""> @item.ProductCategory.Name</a>
                                    </div>
                                    <div class="product-name">
                                        <a href="~/home/#" title=""> @item.Name</a>
                                    </div>
                                    <div class="price">
                                        <span class="sale">@item.SalePrice.ToString("0,0", System.Globalization.CultureInfo.CreateSpecificCulture("el-GR")) &#8363</span>
                                        <span class="regular">@item.OriginalPrice.ToString("0,0", System.Globalization.CultureInfo.CreateSpecificCulture("el-GR")) &#8363</span>
                                    </div>
                                </div><!-- /.box-content -->
                                <div class="box-bottom">
                                    <div class="btn-add-cart">
                                        <a asp-action="Create" asp-controller="Carts" asp-route-id="@item.Id" title="">
                                            <img src="~/home/images/icons/add-cart.png" alt="">Chọn mua
                                        </a>
                                    </div>
                                    <div class="compare-wishlist">
                                        <a href="~/home/#" class="compare" title="">
                                            <img src="~/home/images/icons/compare.png" alt="">So sánh
                                        </a>
                                        <a href="~/home/#" class="wishlist" title="">
                                            <img src="~/home/images/icons/wishlist.png" alt="">Yêu thích
                                        </a>
                                    </div>
                                </div><!-- /.box-bottom -->
                            </div><!-- /.imagebox -->
                        </div>
                    </div><!-- /.col-lg-3 col-sm-6 -->
                }

